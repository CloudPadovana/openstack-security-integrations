%triggerin -n openstack-auth-shib -- openstack-dashboard
if [ ! $1 == 2 ]; then
    #
    # Register custom authentication plugin
    # Register new middleware
    #
    printf "
HORIZON_CONFIG['user_home'] = 'openstack_auth_shib.utils.get_user_home'       #RPM-Trigger: do not edit this line
AUTHENTICATION_PLUGINS = [                                                    #RPM-Trigger: do not edit this line
    'openstack_auth_shib.backend.SKeyPluginFactory',                          #RPM-Trigger: do not edit this line
    'openstack_auth.plugin.password.PasswordPlugin',                          #RPM-Trigger: do not edit this line
    'openstack_auth.plugin.token.TokenPlugin'                                 #RPM-Trigger: do not edit this line
]                                                                             #RPM-Trigger: do not edit this line
AUTHENTICATION_URLS = ['openstack_auth_shib.urls']                            #RPM-Trigger: do not edit this line
INSTALLED_APPS = [                                                            #RPM-Trigger: do not edit this line
    'openstack_dashboard',                                                    #RPM-Trigger: do not edit this line
    'django.contrib.contenttypes',                                            #RPM-Trigger: do not edit this line
    'django.contrib.auth',                                                    #RPM-Trigger: do not edit this line
    'django.contrib.sessions',                                                #RPM-Trigger: do not edit this line
    'django.contrib.messages',                                                #RPM-Trigger: do not edit this line
    'django.contrib.staticfiles',                                             #RPM-Trigger: do not edit this line
    'django.contrib.humanize',                                                #RPM-Trigger: do not edit this line
    'django_pyscss',                                                          #RPM-Trigger: do not edit this line
    'openstack_dashboard.django_pyscss_fix',                                  #RPM-Trigger: do not edit this line
    'compressor',                                                             #RPM-Trigger: do not edit this line
    'horizon',                                                                #RPM-Trigger: do not edit this line
    'openstack_auth',                                                         #RPM-Trigger: do not edit this line
    'openstack_auth_shib',                                                    #RPM-Trigger: do not edit this line
]                                                                             #RPM-Trigger: do not edit this line
NOTIFICATION_TEMPLATE_DIR = '/etc/openstack-auth-shib/notifications'          #RPM-Trigger: do not edit this line

" >> /etc/openstack-dashboard/local_settings
    
    #
    # Replace attribe map file
    #
    if [ ! -e /etc/shibboleth/attribute-map.xml.dist ] ; then
        mv -f /etc/shibboleth/attribute-map.xml /etc/shibboleth/attribute-map.xml.dist
    fi
    ln -sf /usr/share/openstack-auth-shib/attribute-map.xml /etc/shibboleth/attribute-map.xml

fi

%triggerun -n openstack-auth-shib -- openstack-dashboard
if [ $1 == 0 -o $2 == 0 ] ; then

    sed -i -e '/RPM-Trigger/d' /etc/openstack-dashboard/local_settings

    if [ -e /etc/shibboleth/attribute-map.xml.dist ] ; then
        cp -f /etc/shibboleth/attribute-map.xml.dist /etc/shibboleth/attribute-map.xml
    fi
fi





%package -n keystone-skey-auth
Summary: Keystone authentication plugin for secret token
Group: System Environment/Libraries
Requires: python-crypto
Requires: python-keystone

%description -n keystone-skey-auth
Keystone authentication plugin for secret token

%files -n keystone-skey-auth
%defattr(-,root,root)
%config(noreplace) /etc/keystone-skey-auth/policy.json
%dir %{python_sitelib}/keystone_skey_auth
%{python_sitelib}/keystone_skey_auth/*.py
%{python_sitelib}/keystone_skey_auth/*.pyc
%{python_sitelib}/keystone_skey_auth/*.pyo
%{python_sitelib}/*.egg-info

%triggerin -n keystone-skey-auth -- openstack-keystone
if [ ! $1 == 2 ]; then
    #
    # Replace policy file
    #
    if [ ! -e /etc/keystone/policy-origin.json ] ; then
        mv -f /etc/keystone/policy.json /etc/keystone/policy-origin.json
    fi
    ln -sf /etc/keystone-skey-auth/policy.json /etc/keystone/policy.json
fi

%triggerun -n keystone-skey-auth -- openstack-keystone
if [ $1 == 0 -o $2 == 0 ] ; then
    if [ -e /etc/keystone/policy-origin.json ] ; then
        mv -f /etc/keystone/policy-origin.json /etc/keystone/policy.json
    fi
fi










